{
  "name": "rest-express",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all lint format clean && tsc",
    "start": "node src",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon src -e ts,json --exec 'yarn run dev'",
    "lint": "eslint src",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "run:dev": "docker compose up"
  },
  "dependencies": {
    "@prisma/client": "4.13.0",
    "express": "4.18.2",
    "express-validator": "^7.0.1",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "18.16.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "cors": "^2.8.5",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "prisma": "4.13.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
